<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Foodlist">


    <Button
        android:id="@+id/localBtn"
        android:layout_width="218dp"
        android:layout_height="123dp"
        android:text="@string/local_btn"
        app:layout_constraintBottom_toTopOf="@+id/cnBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.883"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        android:textSize="20dp"
        />

    <ImageView
        android:id="@+id/imagelocal"
        android:layout_width="131dp"
        android:layout_height="123dp"
        app:layout_constraintBottom_toTopOf="@+id/imagecn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.057"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/local" />

    <ImageView
        android:id="@+id/imagecn"
        android:layout_width="133dp"
        android:layout_height="122dp"
        app:layout_constraintBottom_toTopOf="@+id/imagewest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.057"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ch" />

    <ImageView
        android:id="@+id/imagewest"
        android:layout_width="133dp"
        android:layout_height="126dp"
        app:layout_constraintBottom_toTopOf="@+id/imagejk"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.057"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/west" />

    <ImageView
        android:id="@+id/imagesoutheast"
        android:layout_width="133dp"
        android:layout_height="119dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.915"
        app:srcCompat="@drawable/southeast" />

    <ImageView
        android:id="@+id/imagejk"
        android:layout_width="132dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toTopOf="@+id/imagesoutheast"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/jk" />

    <Button
        android:id="@+id/cnBtn"
        android:layout_width="222dp"
        android:layout_height="121dp"
        android:text="@string/cn_btn"
        app:layout_constraintBottom_toTopOf="@+id/westBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.894"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        android:textSize="20dp"/>

    <Button
        android:id="@+id/westBtn"
        android:layout_width="222dp"
        android:layout_height="126dp"
        android:text="@string/west_btn"
        app:layout_constraintBottom_toTopOf="@+id/jkBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.883"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        android:textSize="20dp"/>

    <Button
        android:id="@+id/jkBtn"
        android:layout_width="222dp"
        android:layout_height="117dp"
        android:text="@string/jk_btn"
        app:layout_constraintBottom_toTopOf="@+id/southeastBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.993"
        android:textSize="20dp"/>

    <Button
        android:id="@+id/southeastBtn"
        android:layout_width="223dp"
        android:layout_height="117dp"
        android:text="@string/southeast_btn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.912"
        android:textSize="19dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>